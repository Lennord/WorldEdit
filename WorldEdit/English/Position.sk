command //pos1 [<text>]:
	executable by: player and console
	aliases: worldedit:/pos1
	description: &fWorldEdit command //pos1.
	trigger:
		if sender = player:
			WorldEditPos1Cmd(player)
		if sender = console:
			send "&4| &8x &cYou must be a player to use this command." to console
			stop

on command "//pos1" or "worldedit:/pos1":
	if sender = player:
		if player doesn't have permission "worldedit.*" or "worldedit.pos1":
			cancel event
			send "&4| &8x &cYou do not have permission to use this command." to player
			stop

